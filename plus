import sys
an = ans = ansa = ansb = anse = ansf = 0.0
ansc = ansd = 1.0

for line in sys.stdin:
       ans  += 1 
       ansa += float(line)
       ansb -= float(line)
       ansc *= float(line)
       ansd /= float(line)
       anse  = ansa/ans
print("和",ansa)
print("差",ansb)
print("積",ansc)
print("商",ansd)
print("平均",anse)

           
